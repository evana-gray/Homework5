MeasureAndSpread1 <- list(
combined |> summarize(across(age,.fns = fxlist,.names = "{.col}_{.fn}")),
combined |> summarize(across(absences.x,.fns = fxlist,.names = "{.col}_{.fn}")),
combined |> summarize(across(G3.x,.fns = fxlist,.names = "{.col}_{.fn}"))
)
MeasureAndSpread2
